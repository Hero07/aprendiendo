function Saludar(){
    var saludo = "";
    var tiempo = new Date().getHours();
    
    if(tiempo <12){
        saludo = "Â¡Buenos Dias!";
    }else if(tiempo < 18){
        saludo = "Buenas Tardes";
    }else{
        saludo = "Buenas Noches";
    }
    document.getElementById("saludo").innerHTML = saludo;
}
